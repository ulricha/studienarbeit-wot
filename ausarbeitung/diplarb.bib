@INPROCEEDINGS{Duan2009,
  author = {Duan, Dongsheng and Li, Yuhua and Jin, Yanan and Lu, Zhengding},
  title = {Community mining on dynamic weighted directed graphs},
  booktitle = {CNIKM '09: Proceeding of the 1st ACM international workshop on Complex
