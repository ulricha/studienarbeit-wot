@INPROCEEDINGS{Leskovec2008,
  author = {Leskovec, Jure and Lang, Kevin J. and Dasgupta, Anirban and Mahoney,
	Michael W.},
  title = {Statistical properties of community structure in large social and
	information networks},
  booktitle = {WWW '08: Proceeding of the 17th international conference on World
